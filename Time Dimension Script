    #"Multiplied Column2" = Table.TransformColumns(#"Inserted Integer-Division7", {{"Integer-Division", each _ * 10, type number}}),
    #"Renamed Columns14" = Table.RenameColumns(#"Multiplied Column2",{{"Integer-Division", "Minute Bin 10"}}),
    #"Duplicated Column2" = Table.DuplicateColumn(#"Renamed Columns14", "Time", "Time - Copy"),
    #"Reordered Columns" = Table.ReorderColumns(#"Duplicated Column2",{"Time", "Time - Copy", "Hour", "Minute", "Second", "AM/PM", "Hour Label", "Minute Label", "Second Label", "TimeKey", "Hour Bin 12", "Hour Bin 8", "Hour Bin 6", "Hour Bin 4", "Hour Bin 3", "Hour Bin 2", "Minute Bin 30", "Minute Bin 15", "Minute Bin 10"}),
    #"Extracted First Characters" = Table.TransformColumns(#"Reordered Columns", {{"Time - Copy", each Text.Start(Text.From(_, "en-IE"), 5), type text}}),
    #"Added Suffix" = Table.TransformColumns(#"Extracted First Characters", {{"Time - Copy", each _ & ":00", type text}}),
