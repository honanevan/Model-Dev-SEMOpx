    #"Changed Type5" = Table.TransformColumnTypes(#"Inserted Merged Column",{{"Merged", type time}}),
    #"Renamed Columns15" = Table.RenameColumns(#"Changed Type5",{{"Merged", "Time 15 minute bin"}}),
    #"Reordered Columns2" = Table.ReorderColumns(#"Renamed Columns15",{"Time", "Time - Copy", "Time 15 minute bin", "Hour", "Minute - Copy", "Minute", "Second", "AM/PM", "Hour Label", "Minute Label", "Second Label", "TimeKey", "Hour Bin 12", "Hour Bin 8", "Hour Bin 6", "Hour Bin 4", "Hour Bin 3", "Hour Bin 2", "Minute Bin 30", "Minute Bin 15", "Minute Bin 10"}),
    #"Removed Columns3" = Table.RemoveColumns(#"Reordered Columns2",{"Minute - Copy"}),
    #"Renamed Columns16" = Table.RenameColumns(#"Removed Columns3",{{"Time - Copy", "Time 1 minute bin"}}),
    #"Inserted Merged Column1" = Table.AddColumn(#"Renamed Columns16", "Time 30 minute bin", each Text.Combine({[Hour Label], Text.From([Minute Bin 30], "en-IE")}, ":"), type text),
    #"Changed Type6" = Table.TransformColumnTypes(#"Inserted Merged Column1",{{"Time 30 minute bin", type time}}),
    #"Inserted Merged Column2" = Table.AddColumn(#"Changed Type6", "Time 10 Minute bin", each Text.Combine({[Hour Label], Text.From([Minute Bin 10], "en-IE")}, ":"), type text),
    #"Changed Type7" = Table.TransformColumnTypes(#"Inserted Merged Column2",{{"Time 10 Minute bin", type time}}),
    #"Reordered Columns3" = Table.ReorderColumns(#"Changed Type7",{"Time", "Time 1 minute bin", "Time 10 Minute bin", "Time 15 minute bin", "Time 30 minute bin", "Hour", "Minute", "Second", "AM/PM", "Hour Label", "Minute Label", "Second Label", "TimeKey", "Hour Bin 12", "Hour Bin 8", "Hour Bin 6", "Hour Bin 4", "Hour Bin 3", "Hour Bin 2", "Minute Bin 30", "Minute Bin 15", "Minute Bin 10"}),
    #"Renamed Columns17" = Table.RenameColumns(#"Reordered Columns3",{{"Time", "Time 1 second bin"}})
